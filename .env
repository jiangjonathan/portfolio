# Admin secret for accessing edit mode (frontend)
# Usage: https://yoursite.com?admin=YOUR_SECRET_HERE
VITE_ADMIN_KEY=your-secret-key-change-this

# Admin API token for backend operations (must match worker's ADMIN_TOKEN)
# Used for delete and other admin API operations
VITE_ADMIN_API_TOKEN=your-admin-api-token-here

# Local worker API URL (worker runs on port 8787 when using npm run dev)
VITE_WORKER_API_URL=http://localhost:8787
